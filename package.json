{
  "name": "outframer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --port 8788",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create outframer-waitlist",
    "db:migrate": "wrangler d1 execute outframer-waitlist --file=./db/schema.sql",
    "db:migrate:local": "wrangler d1 execute outframer-waitlist --local --file=./db/schema.sql"
  },
  "dependencies": {
    "hono": "^4.6.0",
    "marked": "^11.1.1",
    "dompurify": "^3.0.8",
    "isomorphic-dompurify": "^2.9.0",
    "nanoid": "^5.0.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241205.0",
    "typescript": "^5.7.0",
    "wrangler": "^4.56.0"
  }
}

